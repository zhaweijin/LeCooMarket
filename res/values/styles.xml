<?xml version="1.0" encoding="UTF-8"?>
<resources>

    <style name="IWRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:minHeight">12.0dip</item>
        <item name="android:maxHeight">12.0dip</item>
        <item name="android:progressDrawable">@drawable/ratingbar_small</item>
        <item name="android:isIndicator">true</item>
    </style>
    

    
<style name="comment_IWRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:indeterminateDrawable">@drawable/ratingbar_small</item>
        <item name="android:progressDrawable">@drawable/ratingbar_small</item>
        <item name="android:isIndicator">true</item>
        <item name="android:minWidth">16.0px</item>
        <item name="android:minHeight">15.0dip</item>
        <item name="android:maxWidth">16.0px</item>
        <item name="android:maxHeight">15.0dip</item>
    </style>

    <style name="IWRatingBarLarge" parent="@android:style/Widget.RatingBar">
        <item name="android:maxHeight">24.0dip</item>
        <item name="android:indeterminateDrawable">@drawable/ratingbar_large</item>
        <item name="android:progressDrawable">@drawable/ratingbar_large</item>
        <item name="android:minHeight">24.0dip</item>
        <item name="android:thumb">@null</item>
        <item name="android:isIndicator">false</item>
    </style>

    <style name="IWRatingBarPingrun" parent="@android:style/Widget.RatingBar">
        <item name="android:maxHeight">42.0dip</item>
        <item name="android:indeterminateDrawable">@drawable/ratingbar_large</item>
        <item name="android:progressDrawable">@drawable/ratingbar_large</item>
        <item name="android:minHeight">42.0dip</item>
        <item name="android:thumb">@null</item>
        <item name="android:isIndicator">false</item>
    </style>

    <style name="IWRatingBarSmall" parent="@android:style/Widget.RatingBar">
        <item name="android:indeterminateDrawable">@drawable/ratingbar_large</item>
        <item name="android:progressDrawable">@drawable/ratingbar_large</item>
        <item name="android:thumb">@null</item>
        <item name="android:isIndicator">false</item>
    </style>

    <style name="ErrorTypeRadio">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:textSize">@dimen/font_size_m</item>
        <item name="android:layout_height">30dip</item>
        <item name="android:button">@drawable/payment_radiobtn</item>
    </style>

    <!-- splash day -->
    <style name="IWProgressBarStar" parent="@android:style/Widget.RatingBar">
        <item name="android:maxWidth">22.0px</item>
        <item name="android:maxHeight">22.0px</item>
        <item name="android:indeterminateDrawable">@drawable/progressbar_star</item>
        <item name="android:progressDrawable">@drawable/progressbar_star</item>
        <item name="android:minWidth">22.0px</item>
        <item name="android:minHeight">22.0px</item>
        <item name="android:thumb">@null</item>
        <item name="android:isIndicator">false</item>
    </style>

    <style name="WarningTheme" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/popup</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="TrafficWarningTheme" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@drawable/popup</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
 <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
 <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
 <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/black</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
 <!-- 模糊 -->
    </style>

    <style name="seekbarmystyle" parent="android:style/Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/spladyseekbar</item>
        <item name="android:maxHeight">5dip</item>
        <item name="android:minHeight">5dip</item>
        <item name="android:thumb">@drawable/and</item>
    </style>

    
     <style name="install_checkbox" parent="@android:style/Widget.CompoundButton.CheckBox">
     <item name="android:button">@drawable/install_checkbox</item>
     </style>
</resources>